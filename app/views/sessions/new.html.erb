<div class="container">
  <%= form_tag sessions_path do %>
      <div class=" form-group">
        <%= label_tag :email %>
        <%= text_field_tag :email, nil, :placeholder => 'please enter user email address', class: 'form-control' %>
      </div>
      <div class=" form-group">
        <%= label_tag :password %>
        <%= text_field_tag :password, nil, :placeholder => 'please enter user password', class: 'form-control' %>
      </div>
      <%= submit_tag 'Sign In', :class => 'btn btn-primary' %>
  <% end %>
</div>